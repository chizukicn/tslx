{
  "name": "tslx",
  "version": "0.0.2",
  "description": "",
  "repository": "chizukicn/tslx",
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "vitest dev",
    "lint": "eslint --cache --ext .ts,.js,.mjs,.cjs .",
    "lint:fix": "eslint --cache --ext .ts,.js,.mjs,.cjs . --fix",
    "prepack": "pnpm run build",
    "release": "pnpm test && changelogen --release && npm publish --registry=https://registry.npmjs.org  && git push --follow-tags",
    "test": "pnpm lint && vitest run --coverage"
  },
  "dependencies": {
    "csstype": "^3.1.2"
  },
  "devDependencies": {
    "@curev/eslint-config": "^0.0.9",
    "@vitest/coverage-c8": "^0.30.1",
    "bumpp": "^9.1.0",
    "changelogen": "^0.5.3",
    "eslint": "^8.39.0",
    "prettier": "^2.8.8",
    "typescript": "^5.0.4",
    "unbuild": "^1.2.1",
    "vitest": "^0.30.1"
  },
  "packageManager": "pnpm@8.5.0"
}